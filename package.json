    "description": "OTNode v6 Beta 2",
    "main": "index.js",
    "scripts": {
        "bootstrap-node": "node index.js tools/local-network-setup/.bootstrap_origintrail_noderc",
        "start": "node index.js",
        "dev": "nodemon index.js",
        "prepare": "husky install",
        "ganache": "ganache-cli -e 1000000000000 -i 5777 -p 7545 -l 10000000 -a 30 -m \"aspect ask story desert profit engage tuition leave fade giraffe exclude brief\"",
